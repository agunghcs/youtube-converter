let check720 = withAudioFormats
        .filter((quality) => {
          return (quality.qualityLabel = "720p" || "720p60" || "720p");
        })
        .map((value) => {
          return {
            quality: Number(value.qualityLabel.replace("p", "")),
            itag: value.itag,
            fps: value.fps,
            size: Number(value.contentLength),
          };
        });